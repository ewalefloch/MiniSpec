<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- start : c'est l'id de l'expression racine -->
<Root model="#1">
	<!-- chaque element a un identifiant unique (attribut id) -->
	<!-- Pour associer deux expressions on utilise l'identifiant -->
	<!-- 	exemple : le model de l'entite #30 est pointé par son attribut model avec #1  -->
	<Model id="#1" />
	<Entity model="#1" id="#20" name="Balise"/>
	<Entity model="#1" id="#30" name="Satellite"/>
</Root>